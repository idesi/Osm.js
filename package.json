    "babel-preset-es2015": "6.9.0",
    "chai": "3.5.0",
    "commitizen": "2.8.2",
    "cz-conventional-changelog": "1.1.6",
    "eslint": "3.2.0",
    "eslint-loader": "1.5.0",
    "mocha": "2.5.3",
    "semantic-release": "^4.3.5",
    "webpack": "1.13.1",
    "yargs": "4.8.1"
